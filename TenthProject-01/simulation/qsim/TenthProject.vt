module Tenth_vlg_vec_tst();
// constants                                           
// general purpose registers
reg clear;
reg clk;
reg pause;
reg rst;
// wires                                               
wire [7:0] out_min;
wire [7:0] out_ms;
wire [7:0] out_num;
wire [7:0] out_s;
wire [7:0] out_sel;

wire sampler;                             

// assign statements (if any)                          
Tenth i1 (
// port map - connection between master ports and signals/registers   
	.clear(clear),
	.clk(clk),
	.out_min(out_min),
	.out_ms(out_ms),
	.out_num(out_num),
	.out_s(out_s),
	.out_sel(out_sel),
	.pause(pause),
	.rst(rst)
);

// clear
initial
begin
	clear = 1'b0;
end 

// clk
always
begin
	clk = 1'b0;
	clk = #12500 1'b1;
	#12500;
end 

// pause
initial
begin
	pause = 1'b0;
end 

// rst
initial
begin
	rst = 1'b0;
end 

Tenth_vlg_sample_tst tb_sample (
	.clear(clear),
	.clk(clk),
	.pause(pause),
	.rst(rst),
	.sampler_tx(sampler)
);

Tenth_vlg_check_tst tb_out(
	.out_min(out_min),
	.out_ms(out_ms),
	.out_num(out_num),
	.out_s(out_s),
	.out_sel(out_sel),
	.sampler_rx(sampler)
);
endmodule

